{
  "name": "slidev-theme-codeck",
  "version": "0.0.0",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "homepage": "https://github.com/KonradSzwarc/slidev-theme-codeck#readme",
  "bugs": "https://github.com/KonradSzwarc/slidev-theme-codeck/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/KonradSzwarc/slidev-theme-codeck.git"
  },
  "license": "MIT",
  "author": "Konrad Szwarc <konrad.szwarc.dev@gmail.com> (https://github.com/KonradSzwarc)",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png",
    "format": "prettier --write .",
    "check": "tsc --noEmit"
  },
  "dependencies": {
    "clsx": "2.1.1",
    "markdown-it-ins": "4.0.0",
    "markdown-it-mark": "4.0.0",
    "markdown-it-sub": "2.0.0",
    "markdown-it-sup": "2.0.0",
    "tailwind-merge": "2.4.0"
  },
  "devDependencies": {
    "@slidev/types": "0.49.16",
    "@fontsource-variable/fira-code": "5.0.18",
    "@fontsource-variable/inter": "5.0.19",
    "@slidev/cli": "0.49.16",
    "prettier": "3.3.3",
    "typescript": "5.5.3"
  },
  "peerDependencies": {
    "@fontsource-variable/fira-code": "5.x",
    "@fontsource-variable/inter": "5.x"
  },
  "peerDependenciesMeta": {
    "@fontsource-variable/fira-code": {
      "optional": true
    },
    "@fontsource-variable/inter": {
      "optional": true
    }
  },
  "slidev": {
    "colorSchema": "both",
    "highlighter": "shiki",
    "defaults": {
      "aspectRatio": "16/9",
      "canvasWidth": 960,
      "mdc": true,
      "fonts": {
        "provider": "none",
        "sans": "Inter Variable",
        "mono": "Fira Code Variable"
      }
    }
  }
}
